{
  "version": 2,
  "framework": "nextjs",
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "crons": [
    {
      "path": "/api/cron/weekly-report",
      "schedule": "0 9 * * 1" 
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "preview": true
    }
  }
}
